{
  "name": "mc-api",
  "version": "1.1.1",
  "description": "Minecraft API made by kqzz :D",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.local.js",
    "test": "jest",
    "deploy": "npx serverless deploy --verbose"
  },
  "author": "Kqzz",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "serverless": "^2.25.2",
    "serverless-domain-manager": "^5.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "linebreak-style": [
        "off"
      ],
      "max-len": [
        "off"
      ],
      "camelcase": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "prefer-promise-reject-errors": [
        "off"
      ]
    }
  }
}
